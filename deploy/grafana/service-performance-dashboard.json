{
  "dashboard": {
    "title": "Service Performance",
    "tags": ["gateway", "agentcore", "performance"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Request Latency Distribution",
        "type": "heatmap",
        "gridPos": {"x": 0, "y": 0, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(gateway_http_request_duration_seconds_bucket[5m])) by (le)",
            "format": "heatmap",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "options": {
          "calculate": true,
          "calculation": {},
          "cellGap": 2,
          "color": {
            "mode": "scheme",
            "scheme": "Spectral"
          }
        }
      },
      {
        "id": 2,
        "title": "Latency Percentiles",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(gateway_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.90, rate(gateway_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p90",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(gateway_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.99, rate(gateway_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "D"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Throughput by Method",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (method) (rate(gateway_http_requests_total[5m]))",
            "legendFormat": "{{method}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Request Size Distribution",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(gateway_http_request_size_bytes_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(gateway_http_request_size_bytes_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(gateway_http_request_size_bytes_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "bytes", "label": "Size"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Response Size Distribution",
        "type": "graph",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(gateway_http_response_size_bytes_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(gateway_http_response_size_bytes_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(gateway_http_response_size_bytes_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "bytes", "label": "Size"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Backend Service Request Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 22, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "sum by (service) (rate(gateway_backend_requests_total[5m]))",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Backend Service Error Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 22, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "sum by (service) (rate(gateway_backend_errors_total[5m]))",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "Circuit Breaker State",
        "type": "stat",
        "gridPos": {"x": 0, "y": 28, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "gateway_circuit_breaker_state",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "0", "text": "Closed", "color": "green"},
              {"type": "value", "value": "1", "text": "Open", "color": "red"},
              {"type": "value", "value": "2", "text": "Half-Open", "color": "yellow"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "red"},
                {"value": 2, "color": "yellow"}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Cache Hit Rate",
        "type": "graph",
        "gridPos": {"x": 8, "y": 28, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(gateway_cache_hits_total[5m])) / (sum(rate(gateway_cache_hits_total[5m])) + sum(rate(gateway_cache_misses_total[5m])))",
            "legendFormat": "Hit Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Hit Rate", "max": 1, "min": 0},
          {"format": "short"}
        ]
      },
      {
        "id": 10,
        "title": "Compression Savings",
        "type": "graph",
        "gridPos": {"x": 16, "y": 28, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "rate(gateway_compression_savings_bytes_total[5m])",
            "legendFormat": "{{content_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "Bps", "label": "Bytes/sec"},
          {"format": "short"}
        ]
      }
    ]
  }
}
