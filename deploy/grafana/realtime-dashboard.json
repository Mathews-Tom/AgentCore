{
  "dashboard": {
    "title": "Real-time Dashboard",
    "tags": ["gateway", "agentcore", "realtime"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Current Request Rate",
        "type": "stat",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "sum(rate(gateway_http_requests_total[1m]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1000, "color": "yellow"},
                {"value": 5000, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Current Latency (p95)",
        "type": "stat",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(gateway_http_request_duration_seconds_bucket[1m]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.1, "color": "yellow"},
                {"value": 0.5, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Active Requests",
        "type": "stat",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "gateway_http_requests_active",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 100, "color": "yellow"},
                {"value": 500, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "sum(rate(gateway_http_requests_total{status_code=~\"5..\"}[1m])) / sum(rate(gateway_http_requests_total[1m]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.01, "color": "yellow"},
                {"value": 0.05, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Live Request Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 6, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(gateway_http_requests_total[30s]))",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2
      },
      {
        "id": 6,
        "title": "Live Latency",
        "type": "graph",
        "gridPos": {"x": 12, "y": 6, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(gateway_http_request_duration_seconds_bucket[30s]))",
            "legendFormat": "p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(gateway_http_request_duration_seconds_bucket[30s]))",
            "legendFormat": "p99",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ],
        "lines": true,
        "fill": 0,
        "linewidth": 2
      },
      {
        "id": 7,
        "title": "WebSocket Connections",
        "type": "graph",
        "gridPos": {"x": 0, "y": 14, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "gateway_websocket_connections_active",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Connections"},
          {"format": "short"}
        ],
        "lines": true,
        "fill": 2,
        "linewidth": 1
      },
      {
        "id": 8,
        "title": "WebSocket Message Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 14, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "sum(rate(gateway_websocket_messages_sent_total[30s]))",
            "legendFormat": "Sent/sec",
            "refId": "A"
          },
          {
            "expr": "sum(rate(gateway_websocket_messages_received_total[30s]))",
            "legendFormat": "Received/sec",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "msgps", "label": "Messages/sec"},
          {"format": "short"}
        ],
        "lines": true,
        "fill": 0,
        "linewidth": 2
      },
      {
        "id": 9,
        "title": "Status Code Distribution",
        "type": "piechart",
        "gridPos": {"x": 0, "y": 20, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "sum by (status_code) (rate(gateway_http_requests_total[1m]))",
            "legendFormat": "{{status_code}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "donut",
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          }
        }
      },
      {
        "id": 10,
        "title": "Top Endpoints (Last Minute)",
        "type": "bargauge",
        "gridPos": {"x": 8, "y": 20, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "topk(5, sum by (path) (rate(gateway_http_requests_total[1m])))",
            "legendFormat": "{{path}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "id": 11,
        "title": "Component Health",
        "type": "stat",
        "gridPos": {"x": 16, "y": 20, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "gateway_component_health_status",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "textMode": "name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "1", "text": "Healthy", "color": "green"},
              {"type": "value", "value": "0", "text": "Unhealthy", "color": "red"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Recent Errors",
        "type": "logs",
        "gridPos": {"x": 0, "y": 28, "w": 24, "h": 6},
        "targets": [
          {
            "expr": "{job=\"gateway\"} |= \"ERROR\"",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": true,
          "wrapLogMessage": true
        }
      }
    ]
  }
}
